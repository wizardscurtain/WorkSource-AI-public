{
  "name": "worksource-ai-public",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://kabam.worksource.ai",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
  "build:docs": "react-scripts build && cp docs/CNAME build/CNAME && rm -rf docs/* && cp -r build/* docs/ && cp docs/index.html docs/404.html",
  "deploy:docs": "npm run build:docs && git add docs && git commit -m 'Update docs site' || echo 'No changes to commit' && git push",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^4.0.0"
  }
}
